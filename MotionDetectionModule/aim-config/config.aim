# Use format xxx=xxx for configuration options

# Version must positive integer number
version=1
build_folder=builds

# Brief description for e.g. on the website
description='Calculates the probability of motion in an image stream'
long_description='Calculates the probability of motion in an image stream'
category='Image processing'
#img_url='https://raw.github.com/username/somemodule/aim_config/assets/icon.png'

# Wether the website should add this module
enable='false'

# The application types that are possible: UI, background
application_type=background

# For deployment as Android package
android_package='org.dobots.motiondetectionmodule'
android_url='https://play.google.com/store/apps/details?id=org.dobots.motiondetectionmodule'

# Supported middlewarez
middleware_support_android='false'
middleware_support_yarp='true'
middleware_support_zeromq='true'
middleware_support_ros='true'

# Supported devices
device_support_android='false'
device_support_raspberrypi='true'
device_support_ubuntu='true'
