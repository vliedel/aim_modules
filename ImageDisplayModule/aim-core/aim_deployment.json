{
	"name":"ImageDisplayModule",
	"type":"background",
	"category":"Image processing",
	"description":"Displays an image",
	"supported_middleware":[ "yarp", "zeromq", "ros" ],
	"supported_devices":[ "raspberrypi", "ubuntu" ],
	"enable":"true",
	"android": {
		"package":"org.dobots.imagedisplaymodule",
		"url":"https://play.google.com/store/apps/details?id=org.dobots.imagedisplaymodule"
	},
	"ports": [
		{
			"name":"image",
			"dir":"in",
			"type":"intarray",
			"middleware":"default"
		}
	]
}
