{
	"name":"PictureTransformModule",
	"type":"background",
	"category":"Image processing",
	"description":"Makes a picture look funny",
	"supported_middleware":[ "android", "yarp", "zeromq", "ros" ],
	"supported_devices":[ "android", "raspberrypi", "ubuntu" ],
	"enable":"true",
	"android": {
		"package":"org.dobots.picturetransformmodule",
		"url":"https://play.google.com/store/apps/details?id=org.dobots.picturetransformmodule"
	},
	"ports": [
		{
			"name":"inimage",
			"dir":"in",
			"type":"intarray",
			"middleware":"default"
		},
		{
			"name":"outimage",
			"dir":"out",
			"type":"intarray",
			"middleware":"default"
		}
	]
}
