{
	"name":"PictureSelectModule",
	"version":"1",
	"type":"UI",
	"category":"Image processing",
	"description":"Select a picture on your phone",
	"default_middleware":"android",
	"supported_middleware": ["android"],
	"supported_devices": ["android"],
	"enable":"true",
	"git":"https://github.com/vliedel/aim_modules.git",
	"android": {
		"package":"org.dobots.pictureselectmodule",
		"url":"https://play.google.com/store/apps/details?id=org.dobots.pictureselectmodule"
	},
	"ports": [
		{
			"name":"position",
			"dir":"out",
			"type":"floatarray",
			"middleware":"android"
		},
		{
			"name":"image",
			"dir":"out",
			"type":"intarray",
			"middleware":"android"
		}
	]
}
