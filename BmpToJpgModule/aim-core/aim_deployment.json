{
	"name":"BmpToJpgModule",
	"type":"background",
	"category":"Image processing",
	"description":"Transforms a bitmap to a jpeg as base64 string",
	"default_middleware":"android",
	"supported_middleware": ["android"],
	"supported_devices": ["android"],
	"enable":"true",
	"git":"https://github.com/vliedel/aim_modules.git",
	"android": {
		"package":"org.dobots.bmptojpgmodule",
		"url":"https://play.google.com/store/apps/details?id=org.dobots.bmptojpgmodule"
	},
	"ports": [
		{
			"name":"bmp",
			"dir":"in",
			"type":"intarray",
			"middleware":"android"
		},
		{
			"name":"jpg",
			"dir":"out",
			"type":"string",
			"middleware":"android"
		}
	]
}
